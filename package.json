{
  "name": "open-ui-toolbox",
  "version": "0.0.3",
  "description": "A basic set of react components so I don't need to build the same stuff for every project",
  "main": "dist/bundle.js",
  "scripts": {
    "docs": "cp -R styleguide/* docs/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "styleguide": "./node_modules/react-styleguide-generator/bin/rsg \"components/**/*.jsx\"",
    "start": "npm run styleguide && serve ./styleguide"
  },
  "keywords": [
    "open",
    "react",
    "ui",
    "components"
  ],
  "author": "James Carney",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamescarney3/open-ui-toolbox.git"
  },
  "dependencies": {
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-styleguide-generator": "^1.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.15.2",
    "react-hot-loader": "^1.3.1",
    "serve": "^5.1.3",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
